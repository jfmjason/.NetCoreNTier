

<section class="content-header">
    <h1>
        File
        <small>Approval Process Release</small>
    </h1>

</section>
<!-- Main content -->
<section class="content" >
    <div class="row">
        <div class="col-md-12">
            <div class="box box-default" style="min-height:600px;">
                <div class="box-header">
                    <div class="form-group">
                        <div class="col-md-2">
                            <label>Ref. No.</label>
                            <input id="searchid" class="form-control input-sm text-bold text-navy" />
                            <input id="requestid" class="hidden" />
                        </div>
                        <div class="col-md-10 top-20">
                            <button id="btnsearch" class="btn btn-sm btn-primary "> <i class="fa fa-search"></i> Search</button>
                            <button id="btnclear" class="btn btn-sm btn-default"> <i class="fa fa-eraser"></i> Clear</button>
                        </div>

                    </div>
                </div>
                <hr />
                <div class="box-body" id="releasecontent">
                    <div class="form-group">
                        <div class="col-md-12">
